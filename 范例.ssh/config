# 配置本地服务器gitlab，tianfengs
Host 10.168.1.76                 
    HostName 10.168.1.76
    IdentityFile C:\\Users\\krk10\\.ssh\\id_rsa_tianfengs
    PreferredAuthentications publickey
    User username1

